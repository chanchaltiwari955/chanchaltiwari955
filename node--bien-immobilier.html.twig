<div class="apartment-page">
    {% set slider = drupal_view('property_slider', 'block_1') %}
    {% if slider is not empty %}
        <div class="apartment-slider">
            {{ slider }}
        </div>
    {% endif %}
    <div class="apartment-details container">
        <div class="apartment-inner-sec row">
            <div class="left-sec col-lg-7 col-md-8">
                {% if node.field_apartment_location.value or node.field_apartment_title.value is not empty %}
                    <div class="apartment-title">
                        {% if node.field_apartment_location.value is not empty %}
                            <span class="location">{{ node.field_apartment_location.value }}</span>
                        {% endif %}
                        {% if node.field_apartment_title.value is not empty %}
                            <h1>{{ node.field_apartment_title.value }}</h1>
                        {% endif %}
                    </div>
                {% endif %}
                    <div class="property-rent-sec">
                        {% if node.field_prix.value is not empty %}
                            <div class="rent-price">
                                <p>{{ node.field_prix.fielddefinition.label ~ ':' }}</p>
                                <p>{{ node.field_prix.value }}</p>
                            </div>
                        {% endif %}
                        {% if node.field_loyer.value is not empty %}
                            <div class="rent-price">
                                <p>{{ node.field_loyer.fielddefinition.label ~ ':' }}</p>
                                <p>{{ node.field_loyer.value }}</p>
                            </div>
                        {% endif %}
                        {% if node.field_charges.value is not empty %}
                            <div class="rent-price">
                                <p>{{ node.field_charges.fielddefinition.label ~ ':' }}</p>
                                <p>{{ node.field_charges.value }}</p>
                            </div>
                        {% endif %}
                        {% if node.field_total.value is not empty %}
                            <div class="rent-price">
                                <p>{{ node.field_total.fielddefinition.label ~ ':' }}</p>
                                <p>{{ node.field_total.value }}</p>
                            </div>
                        {% endif %}
                    </div>
                <div class="caracteristiques-sec">
                    {% if node.field_caracteristiques_principal.value is not empty %}
                        <h2>{{ node.field_caracteristiques_principal.value }}</h2>
                    {% endif %}
                    {% if node.field_ndeg_de_pieces.value is not empty %}
                        <div class="caracteristiques-inner">
                            <span class="characteristics-title">
                                <svg width="24" height="24" viewBox="0 0 24 24" fill="none" xmlns="http://www.w3.org/2000/svg">
                                <rect x="3" y="3.40161" width="18" height="18" rx="1" stroke="#041E3D" stroke-linejoin="round"/>
                                <path d="M7.69238 3.47852L7.69238 13.4999" stroke="#041E3D" stroke-linecap="round"/>
                                <path d="M7.69276 21.4785L7.69276 17.5554L9.1543 15.6323" stroke="#041E3D" stroke-linecap="round"/>
                                <path d="M12.5 10.5554L7.6923 10.5554" stroke="#041E3D" stroke-linecap="round"/>
                                <path d="M20.9229 10.5555L16.3844 10.5555L14.3844 12.1709" stroke="#041E3D" stroke-linecap="round"/>
                                </svg>
                                {{ node.field_ndeg_de_pieces.fielddefinition.label }}
                            </span>
                            <span class="characteristics-value">
                                {{ node.field_ndeg_de_pieces.value }}
                            </span>
                        </div>
                    {% endif %}
                    {% if node.field_ndeg_de_chambres.value is not empty %}
                        <div class="caracteristiques-inner">
                            <span class="characteristics-title">
                                <svg width="24" height="24" viewBox="0 0 24 24" fill="none" xmlns="http://www.w3.org/2000/svg">
                                <path d="M4 5C4 4.44772 4.44772 4 5 4H19C19.5523 4 20 4.44772 20 5V19C20 19.5523 19.5523 20 19 20H5C4.44772 20 4 19.5523 4 19V5Z" stroke="#041E3D" stroke-linejoin="round"/>
                                <path d="M6 6H11V8H6V6Z" stroke="#041E3D" stroke-linejoin="round"/>
                                <path d="M13 6H18V8H13V6Z" stroke="#041E3D" stroke-linejoin="round"/>
                                <path d="M4.5 10H19.5" stroke="#041E3D"/>
                                </svg>
                                {{ node.field_ndeg_de_chambres.fielddefinition.label }}
                            </span>
                            <span class="characteristics-value">
                                {{ node.field_ndeg_de_chambres.value }}
                            </span>
                        </div>
                    {% endif %}
                    {% if node.field_ndeg_de_salle_de_bains.value is not empty %}
                        <div class="caracteristiques-inner">
                            <span class="characteristics-title">
                                <svg width="24" height="24" viewBox="0 0 24 24" fill="none" xmlns="http://www.w3.org/2000/svg">
                                <path d="M3 11H21V16C21 18.2091 19.2091 20 17 20H7C4.79086 20 3 18.2091 3 16V11Z" stroke="#041E3D" stroke-linejoin="round"/>
