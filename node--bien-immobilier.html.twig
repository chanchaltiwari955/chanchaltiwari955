                                <path d="M10 5.5C8.89543 5.5 8 6.39543 8 7.5H12C12 6.39543 11.1046 5.5 10 5.5Z" stroke="#041E3D" stroke-linejoin="round"/>
                                <path d="M8.5 20L8.5 21" stroke="#041E3D" stroke-linecap="round"/>
                                <path d="M16.5 20L16.5 21" stroke="#041E3D" stroke-linecap="round"/>
                                </svg>
                                {{ node.field_ndeg_de_salle_de_bains.fielddefinition.label }}
                            </span>
                            <span class="characteristics-value">
                                {{ node.field_ndeg_de_salle_de_bains.value }}
                            </span>
                        </div>
                    {% endif %}
                    {% if node.field_surface_terrain.value is not empty %}
                        <div class="caracteristiques-inner">
                            <span class="characteristics-title">
                                <svg width="24" height="24" viewBox="0 0 24 24" fill="none" xmlns="http://www.w3.org/2000/svg">
                                <path d="M4 4H20V20H4V4Z" stroke="#041E3D" stroke-linecap="round" stroke-linejoin="round" stroke-dasharray="1 2"/>
                                </svg>
                                {{ node.field_surface_terrain.fielddefinition.label }}
                            </span>
                            <span class="characteristics-value">
                                {{ node.field_surface_terrain.value }}
                            </span>
                        </div>
                    {% endif %}
                    {% if node.field_surface_jardin.value is not empty %}
