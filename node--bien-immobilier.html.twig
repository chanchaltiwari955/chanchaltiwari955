                                <path d="M11.832 17.625V21" stroke="#041E3D" stroke-linecap="round" stroke-linejoin="round"/>
                                <path d="M5.2125 16.0111L4.81782 15.7041H4.81782L5.2125 16.0111ZM10.0822 9.75V9.25C9.92792 9.25 9.78226 9.32124 9.68754 9.44303L10.0822 9.75ZM18.4519 16.0111L18.0572 16.318V16.318L18.4519 16.0111ZM13.5822 9.75L13.9769 9.44303C13.8822 9.32124 13.7365 9.25 13.5822 9.25V9.75ZM5.60718 16.318L10.4769 10.057L9.68754 9.44303L4.81782 15.7041L5.60718 16.318ZM6.00185 17.125C5.58578 17.125 5.35173 16.6465 5.60718 16.318L4.81782 15.7041C4.05149 16.6894 4.75363 18.125 6.00185 18.125V17.125ZM17.6626 17.125H6.00185V18.125H17.6626V17.125ZM18.0572 16.318C18.3127 16.6465 18.0786 17.125 17.6626 17.125V18.125C18.9108 18.125 19.6129 16.6894 18.8466 15.7041L18.0572 16.318ZM13.1875 10.057L18.0572 16.318L18.8466 15.7041L13.9769 9.44303L13.1875 10.057ZM13.5822 9.25H10.0822V10.25H13.5822V9.25Z" fill="#041E3D"/>
